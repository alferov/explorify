{
  "name": "Github Showcasify",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Adds the description and the number of stars to starred repositories in the GitHub News Feed",
  "homepage_url": "https://github.com/alferov/github-showcasify",
  "author": "Philipp Alferov",
  "permissions": [
    "http://github.com/*",
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://github.com/*",
        "https://github.com/*"
      ],
      "js": [
        "dist/github-showcasify.js"
      ],
      "run_at": "document_end"
    }
  ]
}
